resource "aws_lambda_layer_version" "<%=name%>" {
    layer_name          = "${local.prefix}--<%=name%>"
    filename            = "${path.module}/../layer/<%=name%>/dist/<%=name%>.zip"
    source_code_hash    = filebase64sha256("${path.module}/../layer/<%=name%>/dist/<%=name%>.zip")
    compatible_runtimes = ["<%=runtime%>"]
}
