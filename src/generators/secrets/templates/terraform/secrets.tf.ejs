<% for (var i = 0; i < data.length; i++) { %>
resource "aws_secretsmanager_secret" "<%=data[i].name%>" {
    name = "<%=data[i].name%>"
    description = "<%=data[i].description%>"
}

resource "aws_secretsmanager_secret_version" "my_password" {
    secret_id     = aws_secretsmanager_secret.<%=data[i].name%>.id
    secret_string = "<%=data[i].secret_string%>"
}
<% } %>